            "schema_validate(ogc_kml23.xsd)",  // Validates against official schema; rejects unauthorized/fictional expansion.
            "closure_audit",                   // Validates that all XML elements are closed/paired.
            "hierarchy_index"                  // Indexes all XML features for strict audit chain-of-custody.
