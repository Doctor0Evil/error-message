    function nanoswarmspecialchar(file: kml_file) {
        ops: [
            "detect_ampersand",                // Locates illegal/unescaped ampersands.
            "detect_angle_brackets"            // Traces non-entity brackets in user-descriptions/names.
        ]
        output: specialchar_fix_map // ALN-legal escape mechanism.
